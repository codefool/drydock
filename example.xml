<drydock xmlns="uri:codefool.org/schemas/drydock" xmlns:tns="uri:codefool.org/schemas/drydock">
	<ship>
	  <guid>1</guid>
	  <name>Constituation Class</name>
	</ship>
    <ship>
      <guid>1</guid>
      <name>Constituation Class</name>
    </ship>
    <component>
		<guid>1</guid>
		<class>bogus</class>
		<name>some system</name>
		<order>0</order>
		<maxDamage>100</maxDamage>
		<disableDamage>25</disableDamage>
		<repairRate>5</repairRate>
		<volume>10</volume>
	  <flamable>
	      <guid>1</guid>
	      <rate>5</rate>
	  </flamable>
      <explosive>
          <guid>1</guid>
          <maxDamage>5</maxDamage>
      </explosive>
    </component>
    <component>
        <guid>1</guid>
        <class>bogus</class>
        <name>some system</name>
        <order>0</order>
        <maxDamage>100</maxDamage>
        <disableDamage>25</disableDamage>
        <repairRate>5</repairRate>
        <volume>10</volume>
    </component>
</drydock>